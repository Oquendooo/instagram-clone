{
  "name": "instagram-clone-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prod": "node server.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "npm run start --prefix frontend",
    "backend": "nodemon --exec babel-node server.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "./node_modules/.bin/babel -d ./build ../ -s",
    "build": "./node_modules/.bin/babel ./ -d ./dist --ignore node_modules,frontend",
    "start": "nodemon --exec babel-node server.js",
    "heroku-postbuild": "cd frontend && npm install --only=dev && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.0",
    "concurrently": "^5.0.0",
    "core-js": "^3.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.3",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@babel/node": "^7.5.0"
  }
}
